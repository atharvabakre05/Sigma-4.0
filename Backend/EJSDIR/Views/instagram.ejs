<%- include("includes/head.ejs") %>
<body>
    <style>
        img{
            height: 300px;
            width: 250px;
        }
    </style>
    <h2>This page belongs to @<%=data.name%></h2>
    <button>Follow</button>
    <button>Message</button>
    <p>Followers: <%= data.followers %> &nbsp; &nbsp; 
        Following: <%= data.following %></p>
    <hr />
    <% for(let post of data.posts) { %>
        <img src = "<%=post.image%>" alt = "car or dog image" />
         <p>Likes: <%= post.likes %> &nbsp; &nbsp; 
        Comments: <%= post.comments %></p>    
    <% } %>

  
</body>
<%- include("includes/footer.ejs") %>
<script src = "/app.js"></script>
</html> 

